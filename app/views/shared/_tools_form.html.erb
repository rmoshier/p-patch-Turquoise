<%= form_for @tool, url {action: obj} do |f| %>
  <%= f.label :title, "Name:" %>
  <%= f.text_field :name %>

  <%= f.label :author, "Quantity:" %>
  <%= f.text_field :quantity %>

  <%= f.label :content, "Photo URL:" %>
  <%= f.text_field :photo_url %>

  <!-- <%= f.hidden_field :in_stock, :value => :quantity %> -->

  <%= f.submit %>
<% end %>
